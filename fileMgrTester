// Import the file I/O to test it
const filemgr = require("./filemgr");
const ReadData = filemgr.ReadData;
const WriteData = filemgr.WriteData;

// Function to test the file I/O function
async function testFileMgr() {
  console.log("Testing WriteData...\n");
  const testData = ["Make food", "Go to gym", "Clean room"];
  await WriteData(testData);

  console.log("Testing ReadData...\n");
  const data = await ReadData();
  console.log("Read data:", data);
}

testFileMgr();
// Test is run with: node testFilemgr.js
